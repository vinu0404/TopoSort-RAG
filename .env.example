# ── LLM Providers ───────────────────────────────────────────

OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ── Default Model Assignments ───────────────────────────────

MASTER_MODEL_PROVIDER=openai
MASTER_MODEL=gpt-4o

RAG_MODEL_PROVIDER=openai
RAG_MODEL=gpt-4o-mini

CODE_MODEL_PROVIDER=openai
CODE_MODEL=gpt-4o-mini

MAIL_MODEL_PROVIDER=openai
MAIL_MODEL=gpt-4o-mini

WEB_MODEL_PROVIDER=openai
WEB_MODEL=gpt-4o-mini

# ── Tavily Web Search API ───────────────────────────────────

TAVILY_API_KEY=your_tavily_api_key_here

# ── Gmail Settings ──────────────────────────────────────────

GMAIL_SENDER_EMAIL=[your_email@gmail.com]  #fallback sender email for Gmail connector (overridden by connected account's email)


JWT_SECRET=generate_a_secure_random_string
JWT_EXPIRY_SECONDS=604800

OAUTH_STATE_SECRET=generate_a_secure_random_string

TOKEN_ENCRYPTION_KEY=generate_fernet_key_here

# ── Google OAuth (Gmail Connector) ─────────────────────────

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Base URL of your backend (local or ngrok)

OAUTH_REDIRECT_BASE=http://localhost:8000

# ── GitHub OAuth ───────────────────────────────────────────

GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# ── Embeddings ─────────────────────────────────────────────

EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=1536

# ── Vector Database (Qdrant) ───────────────────────────────

QDRANT_URL=http://localhost:6333

# ── PostgreSQL ─────────────────────────────────────────────

DATABASE_URL=postgresql+asyncpg://username:password@localhost:5432/mrag

# ── Server Configuration ───────────────────────────────────

HOST=0.0.0.0
PORT=8000
DEBUG=true
CORS_ORIGINS=["http://localhost:3000"]
